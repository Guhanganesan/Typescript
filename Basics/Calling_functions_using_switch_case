 function add()
{
 console.log("Function Calling");
}
add();

function sub(x:any,y:any)
{
    console.log(x-y);
}
sub(20,10);

function mul(x:number,y:number)
{
    return x*y;
}
var z= mul(10,20);
console.log(z);

var pass='*';
switch(pass)
{
    case '+':
    add();
    break;
    case '-':
    sub(50,10);
    break;
    case '*':
    var res=mul(30,20);
    console.log(res);
    break;
}

